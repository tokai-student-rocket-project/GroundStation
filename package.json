{
	"name": "GroundStation",
	"description": "GroundStation",
	"version": "0.1.0",
	"license": "MIT",
	"author": {
		"name": "waiwai"
	},
	"repository": {
		"url": "https://github.com/waiwai/GroundStation.git"
	},
	"main": "dist/main.js",
	"scripts": {
		"dev": "rimraf dist && run-p dev:webpack dev:electron",
		"build": "rimraf dist && cross-env NODE_ENV=\"production\" webpack --progress",
		"dev:webpack": "cross-env NODE_ENV=\"development\" webpack --progress",
		"dev:electron": "wait-on dist/main.js dist/index.html && electronmon ."
	},
	"electronmon": {
		"patterns": [
			"dist/**/*"
		]
	},
	"dependencies": {
		"bulma": "^0.9.4",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.14.2"
	},
	"devDependencies": {
		"@types/node": "^20.4.2",
		"@types/react": "^18.2.15",
		"@types/react-dom": "^18.2.7",
		"cross-env": "^7.0.3",
		"css-loader": "^6.8.1",
		"electron": "^25.3.0",
		"electronmon": "^2.0.2",
		"html-webpack-plugin": "^5.5.3",
		"mini-css-extract-plugin": "^2.7.6",
		"npm-run-all": "^4.1.5",
		"rimraf": "^5.0.1",
		"ts-loader": "^9.4.4",
		"ts-node": "^10.9.1",
		"typescript": "^5.1.6",
		"wait-on": "^7.0.1",
		"webpack": "^5.88.2",
		"webpack-cli": "^5.1.4"
	}
}
